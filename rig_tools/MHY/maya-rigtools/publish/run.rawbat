@echo off
pushd %~dp0
set "INSTALL_PATH=%cd%"
popd
set "MAYA_VERSION=%1"
set "FLAVOR=%2"
if not defined COMPILER_VERSION (
    set "COMPILER_VERSION=2019"
)
